
public class Application {

	public static void main(String[] args) {
		int myNumber;
		long myLong = 8989898;
		
		myNumber = 88;
		myLong = myLong - 1;
		
		System.out.println(" My Number: " + myNumber);
		System.out.println(" My Long: " + myLong);
		
		myLong = myLong + 999999;
		
		System.out.println(" My new Long: " + myLong);
	}

}
