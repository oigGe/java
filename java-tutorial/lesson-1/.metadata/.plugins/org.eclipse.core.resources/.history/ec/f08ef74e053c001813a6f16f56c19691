		int loop = 0;
		boolean running = loop < 5;
		while(running = true) {
			System.out.println("looping...................." + loop);
			
			if(loop == 5) {
				break;
				}
			loop++;
			System.out.println("Running");
		}
		

	while(running = false) {
		System.out.println("finished");
		

	}
	}}

