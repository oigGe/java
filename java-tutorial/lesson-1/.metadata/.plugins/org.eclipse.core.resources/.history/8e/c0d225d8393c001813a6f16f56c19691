import java.util.Scanner;

public class App {
	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		System.out.println("Pleas enter a Command: ");
		String text = input.nextLine();
		
		switch(text) {
		case "start":
			System.out.println("MAchine started");
		}
	}
}
