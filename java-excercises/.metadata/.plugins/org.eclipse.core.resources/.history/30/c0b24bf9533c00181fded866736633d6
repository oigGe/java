import java.util.Scanner;

public class App {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("Enter a number: ");
		int num = in.nextInt();
		for(int i=0; i<=10; i++) {
			System.out.println(num + "" + i + "" + num +i);
			
		}
		while(num <= 10);
	}

}
