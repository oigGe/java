import java.util.Scanner;

public class App {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("Enter a number: ");
		int num = in.nextInt();
		for(int i=1000; i<=0; i--) {
			System.out.println(num + " x " + i + " = " + (num * i));
			
		}
		
	}

}
